{
  "version": 2,
  "buildCommand": "bash ./build.sh",
  "outputDirectory": "VIRALYTIX/frontend/build",
  "installCommand": "npm install --no-save",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/serverless.js"
    }
  ],
  "functions": {
    "api/serverless.js": {
      "memory": 1024,
      "maxDuration": 10
    }
  }
}